{% extends "@bazar/layouts/field.twig" %}

{% block value %}
	{{ value|raw }}

	{% if value == "hedgeDoc" %}
		{% include "@collaborativeTools/hedgedoc.twig" with {'url': url} %}
	{% elseif value == "scrumblr" %}
		{% include "@collaborativeTools/scrumblr.twig" with {'url': url} %}
	{% endif %}
{% endblock %}
